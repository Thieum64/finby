{
  "name": "@hp/sdk-authz",
  "version": "0.1.0",
  "description": "TypeScript client for Hyperush AuthZ service",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -p . --noEmit",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
